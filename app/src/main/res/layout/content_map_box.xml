<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:mapbox="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.akilimo.mobile.views.activities.MapBoxActivity"
    android:id="@+id/mapBoxLyt"
    tools:showIn="@layout/activity_map_box">

    <com.mapbox.mapboxsdk.maps.MapView
        android:id="@+id/mapView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        mapbox:mapbox_cameraTargetLat="-1.2240615"
        mapbox:mapbox_cameraTargetLng="36.9003409"
        mapbox:mapbox_cameraZoom="5"
        mapbox:mapbox_uiRotateGestures="false" />

</RelativeLayout>
